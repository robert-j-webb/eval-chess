{
  "name": "eval-chess",
  "version": "0.0.1",
  "description": "",
  "main": "server/index.ts",
  "scripts": {
    "start": "concurrently  --kill-others-on-fail \"npm:serve\" \"npm:react-start\"  ",
    "serve": "cd server && concurrently --kill-others \"tsc -w --preserveWatchOutput\" \"nodemon bin/index.js\"",
    "lint": "tslint \"(src|server)/*.(ts|tsx)\" --fix",
    "tsc": "tsc",
    "clean": "rimraf bin/*",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch server -e ts --exec npm run dev:start",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robert-j-webb/eval-chess.git"
  },
  "keywords": [
    "eval",
    "chess"
  ],
  "author": "Robert Webb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robert-j-webb/eval-chess/issues"
  },
  "homepage": "https://github.com/robert-j-webb/eval-chess#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint (src|server)/*.(ts|tsx)",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "http-errors": "^1.7.2",
    "http-proxy-middleware": "^0.19.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "24.0.11",
    "@types/node": "^11.11.0",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "@types/shelljs": "^0.8.3",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "ejs": "^2.6.1",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^13.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "prettier": "^1.16.4",
    "react-testing-library": "^6.0.2",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "sinon": "^7.2.3",
    "supertest": "^3.4.2",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.3333"
  }
}
